{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Protocols": "Http2",
        "Url": "http://0.0.0.0:5107"
      },
      "Rest": {
        "Protocols": "Http1",
        "Url": "http://0.0.0.0:5106"
      }
    }
  },
  "Mongo": {
    "ConnectionString": "mongodb://admin:1234567890@localhost:27017",
    "DatabaseName": "FileStorage"
  },
  "S3Storage": {
    "S3AccessKey": "admin",
    "S3SecretKey": "1234567890",
    "S3Url": "http://localhost:9002/"
  },
  "MessageBroker": {
    "Uri": "amqp://localhost:5672",
    "UserName": "admin",
    "Password": "1234567890"
  },
  "StorageSettings": {
    "NotUsingTtl": "1000000",
    "NotCompletedTtl": "40000",
    "BucketByExtensions": [
      {
        "BucketName": "video",
        "Extensions": [
          ".mp4"
        ]
      },
      {
        "BucketName": "images",
        "Extensions": [
          ".png",
          ".jpeg",
          ".jpg"
        ]
      }
    ]
  },
  "CorsSettings": {
    "AllowedOrigins": ""
  },
  "SwaggerSettings": {
    "SchemeName": "MyAuthenticationScheme"
  }
}
